<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="financeManagementSystem.controllers.MainFMSWindow">
   <children>
      <Button fx:id="categorieButton" layoutX="228.0" layoutY="72.0" mnemonicParsing="false" onAction="#accessCategories" text="Categories">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="exitApp" layoutX="266.0" layoutY="276.0" mnemonicParsing="false" onAction="#exitApplication" text="Exit">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="userSettingsButton" layoutX="215.0" layoutY="149.0" mnemonicParsing="false" onAction="#userSettings" text="User Settings">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="save" layoutX="259.0" layoutY="213.0" mnemonicParsing="false" onAction="#saveToFile" text="Save">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
